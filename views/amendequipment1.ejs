</div>
	
	<br><br>
	
	<div class="special">
		<% var arr=[]%>
				<% for(var i=0;i<data[0].length;i++){%>
					<% var x = JSON.parse(data[0][i].answer) %>
					<% for(var j=0;j<x.length;j++){ %>
						<% var y = x[j].name %>
						<% y = y.replace("intent_","") %>
						<% if(! arr.includes(y)){ %>
							<% arr.push(y.trim()) %>
							<span onclick="append(this)">
								<%= y %>
							</span>
						<%}%>
					<%}%>
				<%}%>
		<p id="noequipment">
			No More Equipments To Show..
		</p> 
